@model IEnumerable<StartPage>
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table class="table">
    <thead>
      <tr>
        <th scope="col">NO</th>
        <th scope="col">Başlık</th>
        <th scope="col">Ünvanlar</th>
        <th scope="col">İşlemler</th>
      </tr>
    </thead>
    <tbody>
    @foreach(var i in Model)
    {
        <tr>
            <th>@i.StarPageId</th>
            <th>@i.Title</th>
            <th>@i.Subtitle</th>
            <th><a asp-controller="StartPage" asp-action="Update" asp-route-id="@i.StarPageId" class="btn btn-outline-secondary">Güncelle</a></th>
        </tr>
    }   
    </tbody>
  </table>